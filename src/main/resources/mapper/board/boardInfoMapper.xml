<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.test.secu.board.mapper.BoardInfoMapper">
   <sql id="boardInfoCols">
   		BI_NUM, BI_TITLE, BI_CONTETN, UI_NUM, 
   		BI_FILE_PATH, BI_FILE_NAME, CREDAT, CRETIM,
   		LMODAT, LMOTIM, ACTIVE
   </sql>
   
   <select id="selectBoardInfoByBiNum">
   		SELECT * FROM BOARD_INFO
   		WHERE BI_NUM = #{biNum}
   </select>
</mapper>
